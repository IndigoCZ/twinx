<h1>Přehled Jednot</h1>

<table>
  <tr>
    <th>Název</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @counties.each do |county| %>
  <tr>
    <td><%= county.title %></td>
    <td><%= link_to 'Zobrazit', county %></td>
    <td><%= link_to 'Upravit', edit_county_path(county) %></td>
    <td><%= link_to 'Smazat', county, method: :delete, data: { confirm: 'Jste si jisti?' } %></td>
  </tr>
<% end %>
</table>

